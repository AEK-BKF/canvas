{
	"name": "cnvs/canvas",
	"description": "A Laravel publishing platform",
	"keywords": ["cnvs", "canvas", "laravel", "publishing"],
	"license": "MIT",
	"homepage": "https://cnvs.io",
	"support": {
		"issues": "https://github.com/cnvs/canvas/issues",
		"source": "https://github.com/cnvs/canvas"
	},
	"authors": [
		{
			"name": "Todd Austin",
			"email": "austin.todd.j@gmail.com"
		}
	],
	"require": {
		"php": ">=7.0",
		"ext-json": "*",
		"illuminate/support": "5.5.x|5.6.x|5.7.x|5.8.x"
	},
	"require-dev": {
		"mockery/mockery": "^1.1",
		"orchestra/testbench": "~3.7",
		"phpunit/phpunit": "^7.5"
	},
	"autoload": {
		"psr-4": {
			"Canvas\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Canvas\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit"
	},
	"extra": {
		"laravel": {
			"providers": [
				"Canvas\\CanvasServiceProvider"
			]
		}
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
